{
	"publisher": "leiyunkang",
	"name": "wake-on-lan",
	"displayName": "first-vs-extension",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:LANEquipmentProvider",
		"onCommand:first-vs-extension.helloWorld"
	],
	"main": "./out/extension.js",
	"keywords": [
		"远程唤醒",
		"wake on lan"
	],
	"icon": "media/lan.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/leiyunkang7/first-vs-extension"
	},
	"homepage": "https://github.com/leiyunkang7/first-vs-extension",
	"bugs": "https://github.com/leiyunkang7/first-vs-extension/issues",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "wakeonlan",
					"title": "Wake On Lan",
					"icon": "media/lan.svg"
				}
			]
		},
		"commands": [
			{
				"command": "first-vs-extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "LANEquipmentProvider.refreshEntry",
				"title": "刷新"
			},
			{
				"command": "LANEquipmentProvider.wakeEntry",
				"title": "唤醒"
			}
		],
		"views": {
			"wakeonlan": [
				{
					"id": "LANEquipmentProvider",
					"name": "wakeonlan",
					"icon": "media/lan.svg",
					"contextualTitle": "Wake On Lan"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "LANEquipmentProvider.refreshEntry",
					"when": "view == LANEquipmentProvider",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "LANEquipmentProvider.wakeEntry",
					"when": "view == LANEquipmentProvider && viewItem == equipment",
					"group": "inline"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/lodash-es": "^4.17.4",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.53.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"eslint": "^7.15.0",
		"eslint-plugin-prettier": "^3.3.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"prettier": "^2.2.1",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	},
	"dependencies": {
		"@vue/reactivity": "^3.0.5",
		"local-devices": "^3.1.0",
		"lodash": "^4.17.20",
		"wakeonlan": "^0.1.0"
	}
}
